		"url": "https://github.com/stjarnstrom/vs-code-tromb"
	},
	"bugs": {
		"url": "https://github.com/stjarnstrom/vs-code-tromb/issues"
	},
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"tromb",
		"dark",
		"theme",
		"color-theme",
		"tromb"
	],
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#3c4557",
		"theme": "dark"
	},
	"contributes": {
		"themes": [
			{
				"label": "Tromb",
